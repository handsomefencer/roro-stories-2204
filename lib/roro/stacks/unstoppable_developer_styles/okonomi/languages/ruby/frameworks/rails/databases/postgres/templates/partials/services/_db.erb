

  database:
    image: <%= @env[:base][:db_image][:value] %><%= ":#{@env[:base][:db_image_version][:value]}" %>
    volumes:
      - db_data:/var/lib/postgresql/data
    env_file:
      - ./mise/env/base.env
      - ./mise/env/development.env
      - ./mise/containers/%= @env[:base][:db_image][:value] %>/env/base.env
      - ./mise/containers/database/env/development.env