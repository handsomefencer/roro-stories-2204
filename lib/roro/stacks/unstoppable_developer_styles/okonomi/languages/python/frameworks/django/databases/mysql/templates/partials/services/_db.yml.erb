
  db:
    image: <%= @env[:base][:db_image][:value] %><%= @env[:base][:db_image_tag][:value] %>
    volumes:
      - <%= @env[:base][:db_volume][:value] %>
    env_file:
      - ./mise/env/base.env
      - ./mise/env/development.env
      - ./mise/containers/database/env/base.env
      - ./mise/containers/database/env/development.env
    environment:
      - POSTGRES_NAME=postgres
      - POSTGRES_USER=postgres
      - POSTGRES_PASSWORD=postgres